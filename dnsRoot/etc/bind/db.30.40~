$TTL    60000
@               IN      SOA     ROOT-SERVER.          root.ROOT-SERVER. (
                        2006031201 ; serial
                        28 ; refresh
                        14 ; retry
                        3600000 ; expire
                        0 ; negative cache ttl
                        )
@               IN      NS      ROOT-SERVER.
ROOT-SERVER.	IN	A	30.40.2.1

to.		IN	NS	dnsTo.to.
dnsTo.to.	IN	A	30.40.3.1
